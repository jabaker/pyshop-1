Import multiple csv files as multiple data frames
import os
import glob
import pandas as pd


# Setting up path
path = "//Path/to/your/files/"

# File format
extension = "csv"

# Set path as current working directory
os.chdir(path)

files = [i for i in glob.glob("*.{}".format(extension))]

# Set variable to increment
j = 1

for file in files:
    f = (path + file)
    data = pd.read_csv(f)
    df = ("df" + str(j))
    df = pd.DataFrame(data)
    print(df)

    j += 1

